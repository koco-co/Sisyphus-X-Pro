# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/sisyphus

# Security
SECRET_KEY=your-super-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Redis
REDIS_URL=redis://localhost:6379/0

# MinIO
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_SECURE=false
MINIO_BUCKET=sisyphus

# OAuth - GitHub (Optional - leave empty to disable)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# OAuth - Google (Optional - leave empty to disable)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# OAuth Settings
OAUTH_REDIRECT_URL=http://localhost:8000/api/v1/auth

# CORS - Must be valid JSON array format for pydantic
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# Application
PROJECT_NAME=Sisyphus-X-Pro
API_V1_STR=/api/v1
ENVIRONMENT=development
